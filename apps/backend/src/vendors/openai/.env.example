# OpenAI Configuration
# Copy this to .env in the same directory and fill in your values

# Required: Your OpenAI API key
OPENAI_API_KEY=your_api_key_here

# Optional: OpenAI model to use (default: gpt-3.5-turbo)
OPENAI_MODEL=gpt-3.5-turbo

# Optional: Temperature for responses (0.0 to 1.0, default: 0.7)
OPENAI_TEMPERATURE=0.7

# Optional: Maximum tokens for responses (default: 1000)
OPENAI_MAX_TOKENS=1000

# Optional: Request timeout in milliseconds (default: 30000)
OPENAI_TIMEOUT=30000
